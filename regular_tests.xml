<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Automation">

    <parameter name="device.query" value="@os='android'" />

    <test name="Regular_Test_01">

        <parameter name="application.name" value="unmodified" />

        <classes>
            <class name="tests.RegularTest"/>
        </classes>
    </test>

    <test name="Regular_Test_02">

        <parameter name="application.name" value="modified" />

        <classes>
            <class name="tests.RegularTest"/>
        </classes>
    </test>

</suite>